
BruteForce = function() {
	var _time = 30;
	var _maxTries = 5;
	var _connections = {};
	
};

BruteForce.prototype.addConnection = function(ip){
	sys.log(('Adding ' + ip + 'to connecitons');
	_connections[ip] +=1 ? _connections[ip] != 'undefined' : 1; 
};

BruteForce.prototype.check = function(ip){
	sys.log(('BruteForce check ...');
	_connections[ip] +=1 ? _connections[ip] != 'undefined' : 1; 
};